{
  "name": "tfettig01/InVal",
  "description": "A standalone input validation library.",
  "license": "MIT",
  "authors": [
    {
      "name": "Travis Fettig"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.2"
  },
  "require-dev": {
    "fzaninotto/faker": "1.8.*",
    "infection/infection": "0.9.*",
    "phpunit/phpunit": "7.2.*",
    "squizlabs/php_codesniffer": "3.3.*"
  },
  "autoload": {
    "psr-4": {
      "InVal\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "InValTest\\": "tests/"
    }
  },
  "prefer-stable": true,
  "scripts": {
    "phpcs": "./vendor/bin/phpcs",
    "phpunit": "./vendor/bin/phpunit",
    "infection": "./vendor/bin/infection"
  }
}
