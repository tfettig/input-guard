{
  "name": "tfettig01/InVal",
  "description": "A standalone input validation library.",
  "license": "MIT",
  "authors": [
    {
      "name": "Travis Fettig"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.2"
  },
  "require-dev": {
    "fzaninotto/faker": "1.8.*",
    "infection/infection": "0.9.*",
    "phpmd/phpmd": "2.6.*",
    "phpunit/phpunit": "7.2.*",
    "squizlabs/php_codesniffer": "3.3.*"
  },
  "autoload": {
    "psr-4": {
      "InVal\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "InValTest\\": "tests/"
    }
  },
  "prefer-stable": true,
  "scripts": {
    "infection": "./vendor/bin/infection",
    "infection_build": "./vendor/bin/infection --initial-tests-php-options='-d zend_extension=xdebug.so'",
    "phpcs": "./vendor/bin/phpcs",
    "phpmd": "./vendor/bin/phpmd src/,tests/ text phpmd.xml",
    "phpmd_build": "./vendor/bin/phpmd src/,tests/ text phpmd.xml --reportfile build/phpmd.txt",
    "phpunit": "./vendor/bin/phpunit",
    "phpunit_build": "phpdbg -qrr ./vendor/bin/phpunit"
  }
}
